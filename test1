section .data
section .text
global main
; main function
main:
push ebp
mov ebp, esp
sub esp, 16
push dword 50
pop eax
mov dword [ebp-4], eax
push dword [ebp-4]
; CAST
pop eax
push eax
eax
movsx eax, al
push eax
pop eax
mov byte [ebp-6], al
add esp, 4
push dword [ebp-4]
pop eax
add esp, 16
pop ebp
ret
add esp, 16
pop ebp
ret
section .rodata
