section .data
; int abc
abc: times 4000 db 0
section .text
global main
; main function
main:
push ebp
mov ebp, esp
sub esp, 16
push dword 10
lea ebx, [abc]
push ebx
pop ebx
add ebx, 160
push ebx
pop edx
pop eax
mov dword [edx], eax
push dword [ebp-4]
pop eax
add esp, 16
pop ebp
ret
add esp, 16
pop ebp
ret
section .rodata
