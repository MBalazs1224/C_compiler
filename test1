section .data
section .text
extern printf
global main
; main function
main:
push ebp
mov ebp, esp
lea ebx, [printf]
push ebx
pop ebx
mov ecx, ebx
push dword 50
mov eax, str_1
push eax
call ecx
add esp, 8
push eax
pop eax
push eax
add esp, 4
pop ebp
ret
section .rodata
str_1: db 'h','e','l','l','o',' ','w','o','r','l','d',' ','%','i',10, 0
